          var speakerFields = document.getElementById('speaker-fields');
          var powerbankFields = document.getElementById('powerbank-fields');
          var chargerFields  = document.getElementById('charger-fields');
          var storagedeviceFields = document.getElementById('storagedevice-fields')
          
          if (this.value === 'mobile') {
              watchFields.style.display = 'none';
              mobileFields.style.display = 'block';
              earPhoneFields.style.display = 'none';
              speakerFields.style.display = 'none';
              powerbankFields.style.display = 'none';
              chargerFields.style.display = 'none';
              storagedeviceFields.style.display = 'none';
